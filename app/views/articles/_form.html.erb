<%= javascript_include_tag :ckeditor %>

<%= error_messages_for :article %>
<%= form_for @article do |f| %>


  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title, :size => 70 %>
  </p>
    <%= cktext_area_tag("article[body]", "#{@article.body}")%>
  <p><%= f.submit "Submit" %></p>
<% end %>
