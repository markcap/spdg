<% title "Report" %>

<p>
  <strong>Title:</strong>
  <%=h @report.title %>
</p>
<p>
  <strong>Info:</strong>
  <%=h @report.info %>
</p>
<p>
  <strong>Project:</strong>
  <%=h @report.project_id %>
