<h3><%=@project.name%></h3>

<%= render "tabs"%>

<div class="span-20 my_project_body">
    <div class="right">
    <%=link_to image_tag("../images/icons/edit.gif"), edit_project_path(@project)%>
    <%=spaces(5)%> <%=link_to image_tag("../images/icons/delete.gif"), project_path(@project), 
                    :confirm => 'Are you sure?', :method => :delete%>
    </div>

    <%= @project.info%>
    <br><br><hr>
    Contact information:<br>
    <%= @project.contact_name%><br>
    <%= @project.contact_number%><br>
    <%= @project.contact_email%><br><br>
    Users in Project: <%=@project.users.count%>
</div>