<%= error_messages_for :survey_file %>
<% form_for @survey_file, :html => { :multipart => true } do |f| %>
  <p>
    <%= f.label :survey_id %><br />
    <%= f.text_field :survey_id %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_area :comment %>
  </p>
  <%= f.file_field :document%>
  
  <p><%= f.submit "Submit" %></p>
<% end %>
